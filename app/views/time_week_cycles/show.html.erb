<%- model_class = TimeWeekCycle -%>
<div class="page-header">
  <h1><%=t '.title', default: model_class.model_name.human.titleize %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:activity_class_starts_at).capitalize %></dt>
      <dd><pre class="prettyprint"><%= @time_week_cycle.activity_class_starts_at %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:activity_class_ends_at).capitalize %></dt>
      <dd><pre class="prettyprint"><%= @time_week_cycle.activity_class_ends_at %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:period_started_at).capitalize %></dt>
      <dd><pre class="prettyprint"><%= @time_week_cycle.period_started_at %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:period_ended_at).capitalize %></dt>
      <dd><pre class="prettyprint"><%= @time_week_cycle.period_ended_at %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:activity_id).capitalize %></dt>
      <dd><pre class="prettyprint"><%= @time_week_cycle.activity_id %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', default: t("helpers.links.back")),
              time_week_cycles_path, class: 'btn btn-default'  %>
  <%= link_to t('.edit', default: t("helpers.links.edit")),
              edit_time_week_cycle_path(@time_week_cycle), class: 'btn btn-default' %>
  <%= link_to t('.destroy', default: t("helpers.links.destroy")),
              time_week_cycle_path(@time_week_cycle),
              method: 'delete',
              data: { confirm: t('.confirm', default: t("helpers.links.confirm", default: 'Are you sure?')) },
              class: 'btn btn-danger' %>
</div>