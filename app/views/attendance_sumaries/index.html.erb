<p id="notice"><%= notice %></p>

<h1>Listing Attendance Sumaries</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @attendance_sumaries.each do |attendance_sumary| %>
      <tr>
        <td><%= link_to 'Show', attendance_sumary %></td>
        <td><%= link_to 'Edit', edit_attendance_sumary_path(attendance_sumary) %></td>
        <td><%= link_to 'Destroy', attendance_sumary, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Attendance sumary', new_attendance_sumary_path %>
